{
  "private": true,
  "scripts": {
    "start": "cross-env umi dev",
    "build": "cross-env umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "cross-env UMI_ENV=test umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "^4.10.3",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "egg-init": "^2.3.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-quill": "^1.3.5",
    "react-router": "^6.4.2",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.182",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}